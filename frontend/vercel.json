{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/static-build"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  