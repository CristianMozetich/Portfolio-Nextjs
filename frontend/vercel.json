{
    "version": 2,
    "builds": [
      {
        "src": "/frontend",
        "use": "@vercel/next"
      },
      {
        "src": "/frontend/src/app/[locale]/layout.tsx",
        "use": "@vercel/next"
      },
      {
        "src": "/frontend/public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/frontend/$1" }
    ]
  }
  